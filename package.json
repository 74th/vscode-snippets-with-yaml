{
  "name": "editing-snippets-with-yaml",
  "displayName": "editing snippets with yaml",
  "description": "Editing snippets with YAML format",
  "version": "1.1.0",
  "publisher": "74th",
  "icon": "icon.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.45.0"
  },
  "extensionKind": [
    "ui"
  ],
  "galleryBanner": {
    "color": "#101010",
    "theme": "dark"
  },
  "author": {
    "name": "74th",
    "url": "https://github.com/74th"
  },
  "homepage": "https://github.com/74th/vscode-snippets-with-yaml",
  "repository": {
    "type": "git",
    "url": "https://github.com/74th/vscode-snippets-with-yaml.git"
  },
  "bugs": {
    "url": "https://github.com/74th/vscode-snippets-with-yaml/issues"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onCommand:editing-snippets-by-yaml.configureUserSnippets",
    "onCommand:editing-snippets-by-yaml.configureWorkplaceSnippets"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "editing-snippets-by-yaml.configureUserSnippets",
        "title": "Preferences: Configure User Snippets (YAML)"
      },
      {
        "command": "editing-snippets-by-yaml.configureWorkplaceSnippets",
        "title": "Preferences: Configure Workplace Snippets (YAML)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/prettier": "^2.0.1",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@vscode/vsce": "^2.17.0",
    "jsonc-parser": "^2.2.1",
    "prettier": "^2.0.5",
    "yaml": "^1.10.0",
    "yarn": "^1.22.19"
  }
}